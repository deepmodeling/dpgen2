import os, sys, traceback
from dflow.python import OPIO, TransientError, FatalError
from dflow.python.utils import handle_input_artifact, handle_input_parameter
from dflow.python.utils import handle_output_artifact, handle_output_parameter, handle_lineage
from mocked_ops import MockedRunDPTrain

op_obj = MockedRunDPTrain()
op_obj.key = 'iter-002--run-train-0001'
if op_obj.key.startswith('{'): op_obj.key = None
op_obj.workflow_name = 'block-jhreb'
if __name__ == '__main__':
    input = OPIO()
    input_sign = MockedRunDPTrain.get_input_sign()
    output_sign = MockedRunDPTrain.get_output_sign()
    input['config'] = handle_input_parameter('config', r'''{}''', input_sign['config'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp')
    input['task_name'] = handle_input_parameter('task_name', r'''["task.0000", "task.0001", "task.0002"]''', input_sign['task_name'], int('0001'), r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp')
    input['optional_parameter'] = handle_input_parameter('optional_parameter', r'''{"mixed_type": false, "finetune_mode": "no"}''', input_sign['optional_parameter'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp')
    input['task_path'] = handle_input_artifact('task_path', input_sign['task_path'], int('0001'), r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    input['init_model'] = handle_input_artifact('init_model', input_sign['init_model'], int('0001'), r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    input['init_data'] = handle_input_artifact('init_data', input_sign['init_data'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    input['iter_data'] = handle_input_artifact('iter_data', input_sign['iter_data'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    input['valid_data'] = handle_input_artifact('valid_data', input_sign['valid_data'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    input['optional_files'] = handle_input_artifact('optional_files', input_sign['optional_files'], None, r'/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp', None, n_parts=None, keys_of_parts=None, prefix=None)
    op_obj.tmp_root = '/home/runner/work/dpgen2/dpgen2/tests/block-jhreb/iter-002--run-train-0001/workdir/tmp'
    op_obj.create_slice_dir = False
    op_obj.slices['script'] = int('0001')
    op_obj.slices['model'] = int('0001')
    op_obj.slices['lcurve'] = int('0001')
    op_obj.slices['log'] = int('0001')
    op_obj.pool_size = None
    import signal
    def sigterm_handler(signum, frame):
        print('Got SIGTERM')
        raise RuntimeError('Got SIGTERM')
    signal.signal(signal.SIGTERM, sigterm_handler)
    try:
        try:
            output = op_obj.execute(input)
        except Exception as e:
            if op_obj.outputs:
                op_obj.handle_outputs(op_obj.outputs)
            raise e
    except TransientError:
        traceback.print_exc()
        sys.exit(1)
    except FatalError:
        traceback.print_exc()
        sys.exit(2)
    op_obj.handle_outputs(output)
