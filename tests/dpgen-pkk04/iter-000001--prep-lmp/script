import os, sys, traceback
from dflow.python import OPIO, TransientError, FatalError
from dflow.python.utils import handle_input_artifact, handle_input_parameter
from dflow.python.utils import handle_output_artifact, handle_output_parameter, handle_lineage
from dpgen2.op.prep_lmp import PrepLmp

op_obj = PrepLmp()
op_obj.key = 'iter-000001--prep-lmp'
if op_obj.key.startswith('{'): op_obj.key = None
op_obj.workflow_name = 'dpgen-pkk04'
if __name__ == '__main__':
    input = OPIO()
    input_sign = PrepLmp.get_input_sign()
    output_sign = PrepLmp.get_output_sign()
    input['lmp_task_grp'] = handle_input_parameter('lmp_task_grp', r'''{"py/object": "mocked_ops.MockedExplorationTaskGroup1", "_task_list": [{"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 0", "in.lammps": "mocked 1 input 0"}}, {"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 1", "in.lammps": "mocked 1 input 1"}}, {"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 2", "in.lammps": "mocked 1 input 2"}}, {"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 3", "in.lammps": "mocked 1 input 3"}}, {"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 4", "in.lammps": "mocked 1 input 4"}}, {"py/object": "dpgen2.exploration.task.task.ExplorationTask", "_files": {"conf.lmp": "mocked 1 conf 5", "in.lammps": "mocked 1 input 5"}}]}''', input_sign['lmp_task_grp'], None, r'/home/runner/work/dpgen2/dpgen2/tests/dpgen-pkk04/iter-000001--prep-lmp/workdir/tmp')
    op_obj.tmp_root = '/home/runner/work/dpgen2/dpgen2/tests/dpgen-pkk04/iter-000001--prep-lmp/workdir/tmp'
    op_obj.create_slice_dir = False
    op_obj.slices['task_names'] = None
    op_obj.slices['task_paths'] = None
    op_obj.pool_size = None
    import signal
    def sigterm_handler(signum, frame):
        print('Got SIGTERM')
        raise RuntimeError('Got SIGTERM')
    signal.signal(signal.SIGTERM, sigterm_handler)
    try:
        try:
            output = op_obj.execute(input)
        except Exception as e:
            if op_obj.outputs:
                op_obj.handle_outputs(op_obj.outputs)
            raise e
    except TransientError:
        traceback.print_exc()
        sys.exit(1)
    except FatalError:
        traceback.print_exc()
        sys.exit(2)
    op_obj.handle_outputs(output)
